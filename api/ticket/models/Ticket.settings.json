{
  "connection": "default",
  "collectionName": "ticket",
  "info": {
    "name": "ticket",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "email": {
      "required": true,
      "type": "email"
    },
    "type": {
      "model": "tickettype"
    },
    "checkedIn": {
      "type": "boolean"
    },
    "date": {
      "type": "date"
    },
    "checkedInDate": {
      "type": "date"
    },
    "code": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "order": {
      "model": "order",
      "via": "tickets"
    }
  }
}